<%= bootstrap_form_for(:user, url: session_path(:user), layout: :inline, remote: :true, format: :json) do |f| %>
  <%= f.email_field :email, hide_label: :true, autofocus: true, placeholder: "user@bys.fr" %>
  <%= f.password_field :password, hide_label: :true, autocomplete: "off", placeholder: "Password" %>
  <%= f.submit "Log in"%>
<% end %>
